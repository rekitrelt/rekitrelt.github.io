(($,MODULE_CONFIG)=>{$.fn.plugin=function(){return this.each((function(){let t=$(this),n=t.attr("data-option")||t.attr("data-plugin-option"),i=t.attr("data-plugin").split(/\s+/);for(let o of i)t[o]&&n?t[o].apply(t,getOptions(n,o)):lazyload.load(MODULE_CONFIG[o]).then((function(){n&&t[o].apply(t,getOptions(n,o)),t[o]&&t[o].init&&t[o].init(),window[o]&&window[o].init&&window[o].init()})),t.removeAttr("data-plugin").removeAttr("data-option")}));function getOptions(options){return options&&eval("["+options+"]")}}})(jQuery,MODULE_CONFIG);